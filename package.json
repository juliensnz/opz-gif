{
  "homepage": "https://juliensnz.github.io/opz-gif",
  "name": "opz-giffer",
  "version": "0.4.0",
  "private": false,
  "dependencies": {
    "@giphy/js-fetch-api": "^1.5.8",
    "@giphy/react-components": "^1.0.4",
    "jszip": "^3.3.0",
    "libgif": "^0.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts start",
    "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "npm run build && mv build opz-gif && tar -zcvf opz-gif.tar.gz opz-gif && rm -rf opz-gif"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.1.7",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.1",
    "file-saver": "^2.0.2",
    "gh-pages": "^2.2.0",
    "styled-components": "^5.0.1"
  }
}
